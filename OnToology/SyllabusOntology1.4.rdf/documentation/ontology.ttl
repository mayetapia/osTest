@prefix : <http://purl.org/he/syllabus/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/he/syllabus/ontology#> .

<http://purl.org/he/syllabus/ontology#> rdf:type owl:Ontology ;
                                         <http://purl.org/dc/terms/contributor> "Janneth Chicaiza" ;
                                         <http://purl.org/dc/terms/creator> "Mariela Tapia-Leon" ;
                                         <http://purl.org/dc/terms/rights> "This work is distribution under a Creative Commons Atribution License (https://creativecommons.org/licenses/by/4.0/legalcode)" ;
                                         rdfs:comment "Syllabus ontology provide a consensual knowledge model of syllabus domain. This model is a compilation of the main terms and objects for this particular domain and could be used by Higher Education Institutes."@en ;
                                         rdfs:label "Syllabus ontology" ;
                                         owl:versionInfo 1.4 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty ;
                                   rdfs:range :Syllabus .


###  http://purl.org/dc/terms/rights
<http://purl.org/dc/terms/rights> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://linkedscience.org/teach/ns#studyProgram
<http://linkedscience.org/teach/ns#studyProgram> rdf:type owl:ObjectProperty ;
                                                 rdfs:domain <http://linkedscience.org/teach/ns#Course> ;
                                                 rdfs:range :Syllabus ;
                                                 rdfs:comment "Describes the study program which is associated with the course/seminar." ;
                                                 rdfs:label "study program" ,
                                                            "syllabus" .


###  http://linkedscience.org/teach/ns#teacher
<http://linkedscience.org/teach/ns#teacher> rdf:type owl:ObjectProperty ;
                                            rdfs:domain <http://linkedscience.org/teach/ns#Course> ;
                                            rdfs:range <http://linkedscience.org/teach/ns#Teacher> ;
                                            rdfs:comment "Teacher is an abstract property to relate a teacher to a course/seminar." ;
                                            rdfs:label "teacher" .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:ObjectProperty ;
                                   owl:equivalentProperty <http://schema.org/creator> ;
                                   rdfs:domain :Instructor .


###  http://purl.org/dc/terms/language
<http://purl.org/dc/terms/language> rdf:type owl:ObjectProperty ;
                                    rdfs:domain <http://schema.org/CourseInstance> ;
                                    rdfs:range <http://purl.org/dc/terms/LinguisticSystem> ;
                                    rdfs:comment "A language of the resource." ;
                                    rdfs:label "language" .


###  http://purl.org/dc/terms/tableOfContents
<http://purl.org/dc/terms/tableOfContents> rdf:type owl:ObjectProperty ;
                                           rdfs:domain :Syllabus ;
                                           rdfs:range :Content ;
                                           rdfs:comment "A list of subunits of the resource." ;
                                           rdfs:label "table of contents" .


###  http://purl.org/he/syllabus/ontology#academicDegree
:academicDegree rdf:type owl:ObjectProperty ;
                rdfs:domain [ owl:intersectionOf ( <http://schema.org/Course>
                                                   <http://xmlns.com/foaf/0.1/Person>
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
                rdfs:range <http://vivoweb.org/ontology/core#AcademicDegree> ;
                rdfs:comment "An academic degree at any level, both as reported by individuals for employment and as offered by academic degree programs." ;
                rdfs:label "academic degree" .


###  http://purl.org/he/syllabus/ontology#academicTerm
:academicTerm rdf:type owl:ObjectProperty ;
              rdfs:domain <http://schema.org/CourseInstance> ;
              rdfs:range <http://vivoweb.org/ontology/core#AcademicTerm> ;
              rdfs:comment "An explicit individual academic term, quarter, or semester rather than the generic fall, spring or summer semester." ;
              rdfs:label "academic term" .


###  http://purl.org/he/syllabus/ontology#academicYear
:academicYear rdf:type owl:ObjectProperty ;
              rdfs:domain <http://schema.org/CourseInstance> ;
              rdfs:range <http://vivoweb.org/ontology/core#AcademicYear> ;
              rdfs:comment "An explicit individual period considered by an academic institution to be its primary academic cycle." ;
              rdfs:label "academic year" .


###  http://purl.org/he/syllabus/ontology#approve
:approve rdf:type owl:ObjectProperty ;
         rdfs:domain :Reviewer ;
         rdfs:range :Syllabus ;
         rdfs:comment "Entity who approves the syllabus." ;
         rdfs:label "approved" .


###  http://purl.org/he/syllabus/ontology#assessment
:assessment rdf:type owl:ObjectProperty ;
            rdfs:domain [ owl:intersectionOf ( :Syllabus
                                               :Topic
                                             ) ;
                          rdf:type owl:Class
                        ] ;
            rdfs:range :Assessment ;
            rdfs:comment "An evaluation to decide the quality or importance of something." ;
            rdfs:label "assessment" .


###  http://purl.org/he/syllabus/ontology#assignment
:assignment rdf:type owl:ObjectProperty ;
            rdfs:domain [ owl:intersectionOf ( :Syllabus
                                               :Topic
                                             ) ;
                          rdf:type owl:Class
                        ] ;
            rdfs:range <http://linkedscience.org/teach/ns#Assignment> ;
            rdfs:comment "A piece of work given to someone, typically as part of their studies." ;
            rdfs:label "assignment" .


###  http://purl.org/he/syllabus/ontology#awardedBy
:awardedBy rdf:type owl:ObjectProperty ;
           rdfs:domain <http://vivoweb.org/ontology/core#AcademicDegree> ;
           rdfs:range <http://schema.org/EducationalOrganization> ;
           rdfs:comment "Academic degree awared by an educational institution." ;
           rdfs:label "awared" .


###  http://purl.org/he/syllabus/ontology#classroom
:classroom rdf:type owl:ObjectProperty ;
           rdfs:domain <http://schema.org/CourseInstance> ;
           rdfs:range :Classroom ;
           rdfs:comment "A room in an educational intitution where groups of students are taught." ;
           rdfs:label "classroom" .


###  http://purl.org/he/syllabus/ontology#goal
:goal rdf:type owl:ObjectProperty ;
      rdfs:domain :Syllabus ;
      rdfs:range :GoalStatement ;
      rdfs:comment "The goal statement is one of the most important parts of the definition syllabus. With clearly and concisely stated goals, the rest of the planning process falls naturally into place." ;
      rdfs:label "goal" .


###  http://purl.org/he/syllabus/ontology#hasSchedule
:hasSchedule rdf:type owl:ObjectProperty ;
             rdfs:domain [ owl:intersectionOf ( :Instructor
                                                <http://schema.org/CourseInstance>
                                              ) ;
                           rdf:type owl:Class
                         ] ;
             rdfs:range :Schedule ;
             rdfs:comment "Schedule of the class, laboratory, tutoring, etc." ;
             rdfs:label "schedule" .


###  http://purl.org/he/syllabus/ontology#help
:help rdf:type owl:ObjectProperty ;
      rdfs:domain :Syllabus ;
      rdfs:range :Help ;
      rdfs:comment "Information or advice given to reach success in a course" ;
      rdfs:label "help" .


###  http://purl.org/he/syllabus/ontology#learningResource
:learningResource rdf:type owl:ObjectProperty ;
                  rdfs:domain :Syllabus ;
                  rdfs:range <dct:BibliographicResource> ;
                  rdfs:comment "List of resources of the course." ;
                  rdfs:label "learning resources" .


###  http://purl.org/he/syllabus/ontology#methodology
:methodology rdf:type owl:ObjectProperty ;
             rdfs:domain :Syllabus ;
             rdfs:range :Methodology ;
             rdfs:comment "A system of ways of doing, teaching, or studying something." ;
             rdfs:label "methodology" .


###  http://purl.org/he/syllabus/ontology#policy
:policy rdf:type owl:ObjectProperty ;
        rdfs:domain :Syllabus ;
        rdfs:range :Policy ;
        rdfs:comment "Education policy are the principles and government policy-making in the educational sphere." ;
        rdfs:label "policy" .


###  http://purl.org/he/syllabus/ontology#taxon
:taxon rdf:type owl:ObjectProperty ;
       rdfs:domain :GoalStatement ;
       rdfs:range <http://www.fonte-project.net/ontologies/SICRA.owlTaxonomiaBloom> ;
       rdfs:comment "Bloom’s Taxonomy is a classification of the different objectives and skills that educators set for their students (learning objectives)." ;
       rdfs:label "Bloom taxonomy" .


###  http://purl.org/he/syllabus/ontology#workload
:workload rdf:type owl:ObjectProperty ;
          rdfs:domain :Syllabus ;
          rdfs:range :Workload ;
          rdfs:comment "The amount of work or of working time expected or assigned to do an activity." ;
          rdfs:label "workload" .


###  http://purl.org/spar/fabio/hasManifestation
<http://purl.org/spar/fabio/hasManifestation> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://purl.org/vocab/frbr/core#Work> ;
                                              rdfs:range <http://purl.org/vocab/frbr/core#Manifestation> ;
                                              rdfs:comment "A property linking a particular work to its manifestations" ;
                                              rdfs:label "has manifestation" .


###  http://purl.org/spar/pro/holdsRoleInTime
<http://purl.org/spar/pro/holdsRoleInTime> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                                           rdfs:range <http://purl.org/spar/pro/RoleInTime> ;
                                           rdfs:comment "A property relating an agent to a role that the agent holds." ;
                                           rdfs:label "holds role in time" .


###  http://purl.org/spar/pro/withRole
<http://purl.org/spar/pro/withRole> rdf:type owl:ObjectProperty ;
                                    rdfs:domain <http://purl.org/spar/pro/RoleInTime> ;
                                    rdfs:range <http://purl.org/spar/pro/Role> ;
                                    rdfs:comment "An object property connecting an agent's role in time to a definition of the type of role held by this agent, specified as an instance of the class pro:Role or of one of its sub-classes." ;
                                    rdfs:label "with role" .


###  http://purl.org/vocab/aiiso/schema#part_of
<http://purl.org/vocab/aiiso/schema#part_of> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://schema.org/Organization> ;
                                             rdfs:range <http://schema.org/Organization> ;
                                             rdfs:comment "A foaf Organization may refer to its parent Organizations using part_of." ;
                                             rdfs:label "part of" .


###  http://purl.org/vocab/frbr/core#embodiment
<http://purl.org/vocab/frbr/core#embodiment> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://purl.org/vocab/frbr/core#Expression> ;
                                             rdfs:range <http://purl.org/vocab/frbr/core#Manifestation> ;
                                             rdfs:comment "An expression embodied in a manifestation." ;
                                             rdfs:label "embodiment" .


###  http://purl.org/vocab/frbr/core#realization
<http://purl.org/vocab/frbr/core#realization> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://purl.org/vocab/frbr/core#Work> ;
                                              rdfs:range <http://purl.org/vocab/frbr/core#Expression> ;
                                              rdfs:comment "A work realized through an expression" ;
                                              rdfs:label "realization" .


###  http://schema.org/audience
<http://schema.org/audience> rdf:type owl:ObjectProperty ;
                             rdfs:domain <http://schema.org/Course> ;
                             rdfs:range <http://schema.org/Audience> ;
                             rdfs:comment "Intended audience for an item, i.e. the group for whom the item was created." ;
                             rdfs:label "audience" .


###  http://schema.org/coursePrerequisites
<http://schema.org/coursePrerequisites> rdf:type owl:ObjectProperty ;
                                        owl:equivalentProperty <http://vivoweb.org/ontology/core#hasPrerequisite> ;
                                        rdfs:domain <http://schema.org/Course> ;
                                        rdfs:range <http://schema.org/Course> ;
                                        rdfs:comment "Requirements for taking the Course." ;
                                        rdfs:label "prerequisite" .


###  http://schema.org/creator
<http://schema.org/creator> rdf:type owl:ObjectProperty ;
                            rdfs:domain :Instructor ;
                            rdfs:range :Syllabus ;
                            rdfs:comment "The creator/author of this CreativeWork. This is the same as the Author property for CreativeWork." ;
                            rdfs:label "creator" .


###  http://schema.org/hasCourseInstance
<http://schema.org/hasCourseInstance> rdf:type owl:ObjectProperty ;
                                      rdfs:domain <http://schema.org/Course> ;
                                      rdfs:range <http://schema.org/CourseInstance> ;
                                      rdfs:comment "An offering of the course at a specific time and place or through specific media or mode of study or to a specific section of students." ;
                                      rdfs:label "course instance" .


###  http://schema.org/instructor
<http://schema.org/instructor> rdf:type owl:ObjectProperty ;
                               rdfs:domain <http://schema.org/CourseInstance> ;
                               rdfs:range :Instructor ;
                               rdfs:comment "A person assigned to instruct or provide instructional assistance for the CourseInstance." ;
                               rdfs:label "instructor" ,
                                          "professor" .


###  http://vivoweb.org/ontology/core#hasPrerequisite
<http://vivoweb.org/ontology/core#hasPrerequisite> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain <http://vivoweb.org/ontology/core#Course> ;
                                                   rdfs:range <http://vivoweb.org/ontology/core#Course> ;
                                                   rdfs:comment "The prerequisites for taking the course." ;
                                                   rdfs:label "prerequisite" .


###  http://www.essepuntato.it/2012/04/tvc/atTime
<http://www.essepuntato.it/2012/04/tvc/atTime> rdf:type owl:ObjectProperty ;
                                               rdfs:domain [ owl:intersectionOf ( :Schedule
                                                                                  <http://purl.org/spar/pro/RoleInTime>
                                                                                  <http://schema.org/CourseInstance>
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ] ;
                                               rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval> ;
                                               rdfs:comment "time of the event" ;
                                               rdfs:label "at Time" .


###  http://www.w3.org/2004/02/skos/core#member
<http://www.w3.org/2004/02/skos/core#member> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://www.w3.org/2004/02/skos/core#Collection> ;
                                             rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Collection>
                                                                               <http://www.w3.org/2004/02/skos/core#Concept>
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ] ;
                                             rdfs:comment "Collection has member" ;
                                             rdfs:label "has member" .


###  http://xmlns.com/foaf/0.1/member
<http://xmlns.com/foaf/0.1/member> rdf:type owl:ObjectProperty ;
                                   rdfs:domain <http://schema.org/Person> ;
                                   rdfs:range <http://schema.org/Organization> ;
                                   rdfs:comment "Indicates a member of a Group." ;
                                   rdfs:label "member of a group" .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:DatatypeProperty ;
                                       owl:equivalentProperty <http://schema.org/description> ;
                                       rdfs:domain [ owl:intersectionOf ( <http://linkedscience.org/teach/ns#Assignment>
                                                                          :Assessment
                                                                          :Classroom
                                                                          :Help
                                                                          :Methodology
                                                                          :Policy
                                                                          :Syllabus
                                                                          <http://vivoweb.org/ontology/core#Course>
                                                                        ) ;
                                                     rdf:type owl:Class
                                                   ] ;
                                       rdfs:range xsd:string ;
                                       rdfs:comment "An account of the resource." ;
                                       rdfs:label "description" .


###  http://purl.org/dc/terms/hasVersion
<http://purl.org/dc/terms/hasVersion> rdf:type owl:DatatypeProperty ;
                                      rdfs:domain :Syllabus ;
                                      rdfs:range xsd:string ;
                                      rdfs:comment " A related resource that is a version, edition, or adaptation of the described resource." ;
                                      rdfs:label "version" .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:DatatypeProperty ;
                                 rdfs:domain :Syllabus ;
                                 rdfs:range xsd:string ;
                                 rdfs:comment "A name given to the resource." ;
                                 rdfs:label "title" .


###  http://purl.org/he/syllabus/ontology#bibliographicCitation
:bibliographicCitation rdf:type owl:DatatypeProperty ;
                       rdfs:domain <dct:BibliographicResource> ;
                       rdfs:range xsd:string ;
                       rdfs:comment "A bibliographic reference for the resource." ;
                       rdfs:label "bibliographic citation" .


###  http://purl.org/he/syllabus/ontology#deadline
:deadline rdf:type owl:DatatypeProperty ;
          rdfs:domain [ owl:intersectionOf ( <http://linkedscience.org/teach/ns#Assignment>
                                             :Assessment
                                           ) ;
                        rdf:type owl:Class
                      ] ;
          rdfs:range xsd:dateTime ;
          rdfs:comment "Date the content expires and is no longer useful or available." ;
          rdfs:label "deadline" .


###  http://purl.org/he/syllabus/ontology#durationPerTerm
:durationPerTerm rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Workload ;
                 rdfs:range xsd:nonNegativeInteger ;
                 rdfs:comment "duration of workload per term" ;
                 rdfs:label "duration per term" .


###  http://purl.org/he/syllabus/ontology#durationPerWeek
:durationPerWeek rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Workload ;
                 rdfs:range xsd:nonNegativeInteger ;
                 rdfs:comment "duration of workload per week" ;
                 rdfs:label "duration per week" .


###  http://purl.org/he/syllabus/ontology#educationLevel
:educationLevel rdf:type owl:DatatypeProperty ;
                rdfs:domain <http://vivoweb.org/ontology/core#AcademicDegree> ;
                rdfs:range xsd:string ;
                rdfs:comment "A class of entity, defined in terms of progression through an educational or training context, for which the described resource is intended" ;
                rdfs:label "education level" .


###  http://purl.org/he/syllabus/ontology#number
:number rdf:type owl:DatatypeProperty ;
        rdfs:domain [ owl:intersectionOf ( <http://linkedscience.org/teach/ns#Assignment>
                                           :Assessment
                                           :Classroom
                                         ) ;
                      rdf:type owl:Class
                    ] ;
        rdfs:range xsd:string ;
        rdfs:comment "The number of a class or activity." ;
        rdfs:label "number" .


###  http://purl.org/he/syllabus/ontology#office
:office rdf:type owl:DatatypeProperty ;
        rdfs:domain :Instructor ;
        rdfs:range xsd:string ;
        rdfs:comment "Office of the teacher" ;
        rdfs:label "office" .


###  http://purl.org/he/syllabus/ontology#order
:order rdf:type owl:DatatypeProperty ;
       rdfs:domain :Topic ;
       rdfs:range xsd:string ;
       rdfs:comment "The sequence in which the study topics are arranged." ;
       rdfs:label "order" .


###  http://purl.org/he/syllabus/ontology#requirement
:requirement rdf:type owl:DatatypeProperty ;
             rdfs:domain :Syllabus ;
             rdfs:range xsd:string ;
             rdfs:comment "Requirements for taking the Course. May be completion of another Course or a textual description like \"permission of instructor\". Requirements may be a pre-requisite competency, referenced using AlignmentObject." ;
             rdfs:label "requirement" .


###  http://purl.org/he/syllabus/ontology#resume
:resume rdf:type owl:DatatypeProperty ;
        rdfs:domain :Instructor ;
        rdfs:range xsd:string ;
        rdfs:comment "Short written description of the teacher experience." ;
        rdfs:label "resume" .


###  http://purl.org/he/syllabus/ontology#score
:score rdf:type owl:DatatypeProperty ;
       rdfs:domain [ owl:intersectionOf ( <http://linkedscience.org/teach/ns#Assignment>
                                          :Assessment
                                        ) ;
                     rdf:type owl:Class
                   ] ;
       rdfs:range xsd:string ;
       rdfs:comment "The number of points obtained in an evaluation." ;
       rdfs:label "score" .


###  http://purl.org/he/syllabus/ontology#studyStrategy
:studyStrategy rdf:type owl:DatatypeProperty ;
               rdfs:domain :Methodology ;
               rdfs:range xsd:string ;
               rdfs:comment "Study tips or strategies to improve your studies." ;
               rdfs:label "study strategy" .


###  http://purl.org/he/syllabus/ontology#type
:type rdf:type owl:DatatypeProperty ;
      rdfs:domain [ owl:intersectionOf ( <dct:BibliographicResource>
                                         <http://linkedscience.org/teach/ns#Assignment>
                                         :Assessment
                                         :Classroom
                                         :Instructor
                                         :Topic
                                         <http://vivoweb.org/ontology/core#Course>
                                       ) ;
                    rdf:type owl:Class
                  ] ;
      rdfs:range xsd:string ;
      rdfs:comment "Category or type of something." ;
      rdfs:label "type" .


###  http://purl.org/he/syllabus/ontology#unitPerTerm
:unitPerTerm rdf:type owl:DatatypeProperty ;
             rdfs:domain :Workload ;
             rdfs:range xsd:string ;
             rdfs:comment "unit to measure workload by term" ;
             rdfs:label "unit per term" .


###  http://purl.org/he/syllabus/ontology#unitPerWeek
:unitPerWeek rdf:type owl:DatatypeProperty ;
             rdfs:domain :Workload ;
             rdfs:range xsd:string ;
             rdfs:comment "unit to measure workload by week" ;
             rdfs:label "unit per week" .


###  http://purl.org/he/syllabus/ontology#weight
:weight rdf:type owl:DatatypeProperty ;
        rdfs:domain [ owl:intersectionOf ( <http://linkedscience.org/teach/ns#Assignment>
                                           :Assessment
                                         ) ;
                      rdf:type owl:Class
                    ] ;
        rdfs:range xsd:string ;
        rdfs:comment "weight of the assignment" ;
        rdfs:label "weight" .


###  http://purl.org/vocab/aiiso/schema#code
<http://purl.org/vocab/aiiso/schema#code> rdf:type owl:DatatypeProperty ;
                                          owl:equivalentProperty <http://schema.org/courseCode> ;
                                          rdfs:domain <http://schema.org/Course> ;
                                          rdfs:range xsd:string ;
                                          rdfs:comment "A person assigned to instruct or provide instructional assistance for the CourseInstance." ;
                                          rdfs:label "code" .


###  http://schema.org/courseCode
<http://schema.org/courseCode> rdf:type owl:DatatypeProperty ;
                               rdfs:domain <http://schema.org/Course> ;
                               rdfs:range xsd:string ;
                               rdfs:comment "The identifier for the Course used by the course provider." ;
                               rdfs:label "code" .


###  http://schema.org/courseMode
<http://schema.org/courseMode> rdf:type owl:DatatypeProperty ;
                               rdfs:subPropertyOf owl:topDataProperty ;
                               rdfs:domain <http://schema.org/CourseInstance> ;
                               rdfs:range xsd:string ;
                               rdfs:comment "The medium or means of delivery of the course instance or the mode of study, either as a text label (e.g. \"online\", \"onsite\" or \"blended\"; \"synchronous\" or \"asynchronous\"; \"full-time\" or \"part-time\") or as a URL reference to a term from a controlled vocabulary." ;
                               rdfs:label "course mode" .


###  http://schema.org/description
<http://schema.org/description> rdf:type owl:DatatypeProperty ;
                                rdfs:domain [ owl:intersectionOf ( <http://linkedscience.org/teach/ns#Assignment>
                                                                   :Assessment
                                                                   :Classroom
                                                                   :Help
                                                                   :Methodology
                                                                   :Policy
                                                                   :Syllabus
                                                                   <http://vivoweb.org/ontology/core#Course>
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                                rdfs:range xsd:string ;
                                rdfs:comment "A description of the item." ;
                                rdfs:label "description" .


###  http://schema.org/email
<http://schema.org/email> rdf:type owl:DatatypeProperty ;
                          rdfs:domain <http://schema.org/Agent> ;
                          rdfs:range xsd:string ;
                          rdfs:comment "Email address." ;
                          rdfs:label "email" .


###  http://schema.org/name
<http://schema.org/name> rdf:type owl:DatatypeProperty ;
